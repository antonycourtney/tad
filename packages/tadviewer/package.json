{
  "name": "tadviewer",
  "version": "0.10.0",
  "description": "Tabular data viewer library",
  "main": "dist/tadviewer.js",
  "types": "dist/tadviewer.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "clean": "rimraf dist",
    "build:dir": "mkdirp dist",
    "build:html": "recursive-copy -w html dist",
    "build-assets": "npm run build:dir && npm run build:html",
    "build-dev": "npm run build-assets && webpack --mode development",
    "build-prod": "npm run build-assets && webpack --env prod --mode production",
    "build": "npm run build-prod",
    "tsc": "tsc",
    "watch": "webpack --mode development --watch",
    "watch-prod": "webpack --env prod --mode production --watch"
  },
  "keywords": [
    "relational",
    "sql",
    "database"
  ],
  "author": "Antony Courtney <antony@antonycourtney.com>",
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/core": "3.52.0",
    "@types/he": "^1.1.2",
    "export-to-csv": "^0.2.1",
    "he": "^1.2.0",
    "immutable": "^4.0.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.8.0",
    "oneref": "^0.8.0",
    "react-dnd": "^14.0.5",
    "react-dnd-html5-backend": "^14.1.0",
    "react-select": "^5.2.1",
    "slickgrid-es6": "^3.0.3",
    "url-loader": "^4.1.1",
    "url-regex": "5.0.0",
    "use-deep-compare": "^1.1.0"
  },
  "devDependencies": {
    "@blueprintjs/icons": "^3.31.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-select": "^5.0.0",
    "@types/slickgrid": "^2.1.31",
    "ajv": "^6.9.1",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "image-webpack-loader": "^8.0.1",
    "less": "^3.13.1",
    "less-loader": "^10.2.0",
    "mkdirp": "^1.0.4",
    "node-sass": "^7.0.1",
    "recursive-copy-cli": "1.0.20",
    "resolve-url-loader": "^4.0.0",
    "sass-loader": "^12.4.0",
    "source-map-loader": "^3.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-node-externals": "^3.0.0",
    "aggtree": "^0.10.0",
    "reltab": "^0.10.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "aggtree": "^0.10.0",
    "reltab": "^0.10.0"
  }
}
